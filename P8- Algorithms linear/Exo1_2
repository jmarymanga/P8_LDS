ALGORITHM Sum_Of_All_Elements_Which_Are_Present_In_Either_Of_The_Given_Set_1_2
VAR
i, j,som,elmt: INTEGER;
tab1 : ARRAY_OF INTEGER[4];
tab2 : ARRAY_OF INTEGER[5];
som := 0;
present : BOOLEAN := FALSE;  
htab : HASH_TABLE<INTEGER>;

BEGIN
   htab : HASH_TABLE<INTEGER>;
   htab.insert();

   tab1 := {3, 1, 7, 9}; // inserting the elements directly in the array
   tab2 := {2, 4, 1, 9, 3}; // inserting the elements directly in the array

 
   FOR elmt FROM 0 TO tab1.length-1 STEP 1 DO 
      FOR elmt FROM 0 TO tab2.length-1 STEP 1 DO 

         IF (elmt in tab1 = elmt in tab2) THEN
            present = TRUE;
            htab(elmt) += som ;
            break;
         ELSE
            present = FALSE;
            htab(elmt) = som;
         END_IF

      END_FOR
   END_FOR
   
   Write(som);
   
END